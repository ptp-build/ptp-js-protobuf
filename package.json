{
  "name": "ptp-js-protobuf",
  "version": "0.2.5",
  "description": "im client test",
  "main": "cli/index.js",
  "bin": {
    "minipbjs": "bin/minipbjs"
  },
  "scripts": {
    "test": "tsc --build && tape 'tests/**/test.js'",
    "build": "npm run clean && tsc --build",
    "clean": "rm -rf ./dist",
    "prepublishOnly": "tsc --build && chmod +x bin/minipbjs"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ptp-js-protobuf.git"
  },
  "keywords": [
    "protobuf",
    "protobuf.js",
    "pbjs",
    "minify",
    "minimize"
  ],
  "author": {
    "name": "Jack",
    "email": "crypto.service@protonmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@types/ws": "^8.5.3",
    "bip39": "^3.0.4",
    "bn.js": "^4.12.0",
    "dayjs": "^1.11.5",
    "elliptic": "^6.5.4",
    "ethereum-cryptography": "0.1.3",
    "ethereumjs-wallet": "^1.0.2",
    "md5": "^2.3.0",
    "protobufjs": "6.11.2",
    "uglify-js": "^3.13.6",
    "uuid": "^3.4.0",
    "ws": "^8.9.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.21.0",
    "@jest/console": "^29.2.0",
    "@types/bn.js": "^5.1.1",
    "@types/elliptic": "^6.4.14",
    "@types/jest": "^29.1.2",
    "@types/md5": "^2.3.2",
    "@types/node": "^15.3.0",
    "@types/uuid": "^3.4.0",
    "babel-jest": "^29.2.0",
    "jest": "^29.2.0",
    "tape": "^5.2.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.7.4"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "/tests/.*\\.(test|spec)\\.(js|tsx?)$",
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ]
  }
}
